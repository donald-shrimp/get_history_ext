{
  "name": "履歴取得くん",
  "version": "1.0",
  "description": "俺を信じてくれ…",
  
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "history.png"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "history"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }, 
  "icons": {
    "16": "history.png",
    "48": "history.png",
    "128": "history.png"
  },
  "manifest_version": 2
}